<odoo>
    <data noupdate="1">
        
        <record id="seq_raw_material_assessment" model="ir.sequence">
            <field name="name">Raw Material Assessment Sequence</field>
            <field name="code">data.qc</field>
            <field name="prefix">QC/</field>
            <field name="padding">4</field>
            <field name="number_next">1</field>
            <field name="number_increment">1</field>
        </record>

        <!-- Seq QC -->
        <record id="seq_qc_baku_doc" model="ir.sequence">
            <field name="name">QC Baku Doc Seq</field>
            <field name="code">data.qc.baku.doc</field>
            <!-- <field name="prefix">FRM/QC/RMT/</field> -->
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range">True</field>
        </record>

        <record id="seq_qc_analisa_baku" model="ir.sequence">
            <field name="name">QC Baku Analisa Seq</field>
