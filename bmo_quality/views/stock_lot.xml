<?xml version='1.0' encoding='utf-8'?>
<odoo>
    <record id="view_production_lot_form_inherit_bmo_quality" model="ir.ui.view">
        <field name="name">stock.production.lot.form.inherit.bmo_quality</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='location_id']" position="after">
                <field name="no_analisa_qc" force_save="1" readonly="1"/>
            </xpath>

        </field>
    </record>

</odoo>