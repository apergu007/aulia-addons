<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_po_baku" model="ir.sequence">
            <field name="name">PO Baku Seq</field>
            <field name="code">po.bahan.baku</field>
            <field name="prefix">PO-BK/%(y)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range">True</field>
            <!-- <field name="range_reset">monthly</field> -->
        </record>

        <record id="seq_po_kemas" model="ir.sequence">
            <field name="name">PO Kemas Seq</field>
            <field name="code">po.bahan.kemas</field>
            <field name="prefix">PO-FP/%(y)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range">True</field>
            <!-- <field name="range_reset">monthly</field> -->
        </record>

        <record id="seq_po_lain" model="ir.sequence">
            <field name="name">PO Lain Seq</field>
            <field name="code">po.lain.lain</field>
            <field name="prefix">PO-FS/%(y)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False" />
            <field name="use_date_range">True</field>
            <!-- <field name="range_reset">monthly</field> -->
        </record>
        
    </data>
</odoo>